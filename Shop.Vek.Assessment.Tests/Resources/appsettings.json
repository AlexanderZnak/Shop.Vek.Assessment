{
  "webAppUrl": "https://www.21vek.by/",

  "browserName": "chrome",
  "isElementHighlightEnabled": true,

  "timeouts": {
    "timeoutPageLoad": 70,
    "timeoutCondition": 12,
    "timeoutPollingInterval": 500
  },

  "retry": {
    "number": 4,
    "pollingInterval": 500
  },

  "driverSettings": {
    "chrome": {
      "pageLoadStrategy": "Normal",
      "options": {
        "download.default_directory": "//home//selenium//downloads",
        "disable-popup-blocking": true
      },
      "startArguments": [
        "--no-sandbox",
        "--disable-dev-shm-usage",
        "--disable-gpu"
      ]
    }
  }
}